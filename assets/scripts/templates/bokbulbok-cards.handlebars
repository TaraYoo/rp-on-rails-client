{{#each bokbulboks as |bokbulbok| }}

  <div class="col s12 m4">
    <div class="card hoverable blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title">{{bokbulbok.id}}</span>
        <p class="truncate">Description: {{bokbulbok.description}}</p>
        <p>Used? {{bokbulbok.used}}</p>
      </div>
      <div class="card-action">
        <a class="get-details" id="bokbulbok-{{bokbulbok.id}}">
          Click here to see details, update, or delete
        </a>
      </div>
    </div>
  </div>

{{/each}}
